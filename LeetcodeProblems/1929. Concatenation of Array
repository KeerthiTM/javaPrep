class Solution {
    public int[] getConcatenation(int[] nums) {
        int j=0;
        int[] ans=new int[2*(nums.length)];
        //System.out.println(ans.length);
            for(int i=0; i<nums.length; i++){
                ans[i] = nums[i];
                ans[i+nums.length] = nums[i];
            }
        
        return ans;
    }
}
